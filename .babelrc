{
  "presets": [
    ["@babel/preset-env", {
      modules: false,
      debug: false,
    }]
  ],
  "env": {
    "commonjs": {
      "plugins": [
        ["@babel/plugin-transform-modules-commonjs", { loose: true }]
      ]
    },
    "es": {
      "plugins": [
        ["./.config/plugin/babel/add-import-extension.js", { extension: "mjs" }]
      ]
    },
  }
}
